<!-- views/profile.ejs -->
<!doctype html>
<html>

<head>
	<link rel="stylesheet" href="css/dataTables.bootstrap.min.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/jquery.dataTables.min.js"></script>
	<script src="js/dataTables.bootstrap.min.js"></script>
	<script src="home.js"></script>
	
	<style>
		body {
			word-wrap: break-word;
		}
		.addStyle{
			padding-left: 8%;
		}
	</style>
</head>

<body>
    <%- include('./header'); -%>
    <section class="content-header">
        <h4>
            <div class="row">
                <div class="col-sm-10">
                    <small id="titleTab" style="padding-left: 7%; color: #000;">
                        <i class="glyphicon glyphicon-hand-right"></i>
                        <a href="/home"> Home</a>
                    </small>
                </div>
            </div>
        </h4>
    </section>

    <div id="myModal" class="modal fade" role="dialog">
        <div class="modal-dialog" style="width: 800px;">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Danh sách Admin</h4>
                </div>

                <div class="modal-body">
                    <form class="form-horizontal">
						<div id="dynamicForm">
							
						</div>
					</form>
					
                    <input type="text" class="form-control" id="txtNumberRow" style="display: none">
                </div>

                <div class="modal-footer">
                    <button class="btn btn-danger" id="Close" onclick="Close()">Đóng</button>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <table id="example" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th style="width: 35%; text-align: center">Tên Page</th>
                        <th style="width: 35%; text-align: center">Link</th>
                        <th style="width: 15%; text-align: center">Manager</th>
                        <th style="width: 10%; text-align: center">SL Admin</th>
                        <th style="width: 5%; text-align: center">Option</th>
                    </tr>
                </thead>
                <tbody id="bodyTable">
                </tbody>
            </table>
        </div>
    </div>
</body>

</html>